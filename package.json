{
	"name": "openMindMap",
	"version": "1.0.0",
	"description": "Transform markdown files with #mindmap into interactive mind maps with AI-powered node suggestions and OpenAI-compatible API integration",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"rebuild": "./rebuild.sh",
		"build:core": "node esbuild.test.config.mjs",
		"test": "npm run build:core && node test/test-overlap-verification.js",
		"test:unit": "npm run build:core && node test/unit-tests.js",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Han",
	"license": "MIT",
	"devDependencies": {
		"@types/d3": "^7.4.3",
		"@types/node": "^25.0.3",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.2",
		"obsidian": "^1.11.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"d3": "^7.9.0"
	}
}
